{
  "name": "animedownloader",
  "version": "1.3.1",
  "description": "Anime Downloader - AnimeHub",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "concurrently \"nodemon server.js\" \"tailwindcss -i ./src/input.css -o ./public/css/tailwind.css --watch --minify\"",
    "build:css": "tailwindcss -i ./src/input.css -o ./public/css/tailwind.css --minify",
    "build:ncc": "npx @vercel/ncc build server.js -o dist",
    "build:sea": "node --experimental-sea-config sea-config.json",
    "generate:assets": "node scripts/generate-asset-manifest.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "axios": "^1.13.4",
    "cheerio": "^1.2.0",
    "ejs": "^4.0.1",
    "express": "^5.2.1"
  },
  "devDependencies": {
    "@tailwindcss/cli": "^4.1.18",
    "@vercel/ncc": "^0.38.4",
    "concurrently": "^9.2.1",
    "nodemon": "^3.1.11",
    "postject": "^1.0.0-alpha.6",
    "resedit-cli": "^3.0.1",
    "tailwindcss": "^4.1.18"
  }
}
